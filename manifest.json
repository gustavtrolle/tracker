{
  "name": "Tracker",
  "version": "1.0",
  "description": "Track web activities",
  "permissions": ["background","tabs","activeTab"],
  "background": {
    "scripts": ["jquery.js","background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}